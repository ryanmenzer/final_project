<h1><%=t :hi %></h1>


<!-- <a id="email" href="#">Send Email</a> -->

<%= form_for :email, :html => { :id => "email" } do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
 
  <p>
    <%= f.label :text %><br>
    <%= f.text_area :text %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>
